#include <iostream>
#include <string>
using namespace std;

bool isVowel(char ch) {
    ch = tolower(ch);
    return ch == 'a' || ch == 'o'  || ch == 'e' || ch == 'u' || ch == 'i';
}

int main() {
    string s;
    cin >> s;

    for (char ch : s) {
        if (!isVowel(ch)) {
            cout << '.' << (char)tolower(ch);
        }
    }

    return 0;
}
