#include <bits/stdc++.h>
#include <iostream>
#include <utility>
using namespace std;

int SumFun(vector <int> &arr, int div){
    int sum = 0;
    int n = arr.size();
    for(int i = 0; i<n; i++){
        sum = sum + ceil((double)(arr[i])/ (double)(div));
        
    }

    return sum;
}

int SmallesDivisor(vector <int> &arr, int limit){
    int low = 1;
    int high = *max_element(arr.begin(), arr.end());
    int ans = -1;
    while(low <=high){
        int mid = low + (high-low)/2;

        if(SumFun(arr, mid) <= limit){
            ans = mid;
            high = mid-1;
        }


        else{
            low = mid+1;
        }
    }
    return ans;
}

