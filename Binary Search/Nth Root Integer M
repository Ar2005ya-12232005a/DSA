#include <bits/stdc++.h>
#include <iostream>
#include <utility>
using namespace std;


int func(int mid, int n, int m){
    long long result = 1;
    for(int i = 0; i < n; i++){
         result = result* mid;

         if(result  > m){
        return 2;
    }
}


    if (result == m){
        return 1;
    }

    else{
        return 0;
    }


}

int NthRoot(int n, int m){
    int low = 1;
    int high = m;

    while(low <= high){
      int mid = low + (high-low)/2;

      int midN = func(mid, n, m);

      if(midN == 1){
        return mid;
      }

      else if(midN == 0){
        low = mid+1;
      }

      else{
        high = mid-1;
      }
    }

    return -1;
}


int main(){

}
