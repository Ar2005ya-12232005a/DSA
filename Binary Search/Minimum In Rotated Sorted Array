#include <bits/stdc++.h>
#include <iostream>
#include <utility>
using namespace std;

int MinimumRotatedSortedArray(vector <int> &arr, int n){
    int low = 0;
    int high = n-1;
    int ans = INT_MAX;
    int mid = low + (high - low)/2;

    while(low <= high){
        if(arr[low] <= arr[high]){
            ans = min(ans, arr[low]);
            break;
        }

        else if(arr[low] <= mid){
            ans = min(ans, arr[low]);
            low = mid+1;
        }

        else{
            ans = min(ans,arr[mid]);
            high = mid-1;
        }
    }

    return ans;
}

int main(){

}
