#include <bits/stdc++.h>
#include <iostream>
#include <utility>
using namespace std;


//--------------------BRUTE--------------
int SquareRoot(int n){

    int ans = 1;
    for(int i = 1; i<n; i++){
        if(i*i <=n){
            ans = i;
        
        }else{
            break;
        }
    }

    return ans;
}



//-------------------OPTIMAL----------------------
int SquareRoot2(int n){
    int low =1;
    int high = n;
    int ans = 1;

    while(low <= high){
        int mid = low+ (high- low)/2;

        if(mid * mid <= n){
            ans = mid;
            low = mid+1;
        }

        else{
            high = mid-1;
        }
    }

    return ans;
}


