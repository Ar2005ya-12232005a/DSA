#include <iostream>
using namespace std;

int main()
{

    string a, b;
    cin >> a >> b;

    for (int i = 0; i < a.length(); i++)
    {
        char ch1 = a[i];
        char ch2 = b[i];

        if (ch1 >= 'A' && ch1 <= 'Z')
        {
            ch1 = ch1 + 32;
        }

        if (ch2 >= 'A' && ch2 <= 'Z')
        {
            ch2 = ch2 + 32;
        }

        if (ch1 < ch2)
        {
            cout<<-1<<endl;
            return 0;
        }
        else if (ch1 > ch2)
        {
            cout<<1<<endl;
            return 0;
        }
        
    }
    cout<<0<<endl;
    return 0;
}
