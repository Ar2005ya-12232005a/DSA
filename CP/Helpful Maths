#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main(){

    string s;
    cin>>s;

    vector <int> name;
    for(int i = 0; i< s.size(); i+=2){
        name.push_back(s[i]-'0');
    }


    sort(name.begin(), name.end());

    for(int i = 0; i< name.size(); i++){
        cout<<name[i];
        if(i != name.size()-1){
            cout<<'+';
        }
    }

    cout<<endl;
    
    return 0;
}
