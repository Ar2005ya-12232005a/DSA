// Euclidean algorithm

#include <iostream>
using namespace std;

void Euclidean(int a , int b){
    int gcd = 0;
    while(a>0 && b>0){
        if (a>b){
            a = a%b;
       
        }else {
            b = b%a;
        }

        if (a==0){
            gcd = b;

        }else{
            gcd = a;
        }
    }
    cout<<"The gcd is: "<<gcd<<endl;
}

int main(){

    int m , n ;
    cout<<"Enter value of m: ";
    cin>>m;
    cout<<"Enter the value of n: ";
    cin>>n;

    Euclidean(m,n);
    



    return 0;
}
