#include <bits/stdc++.h>
#include <iostream>
#include <utility>
using namespace std;

//-----------------------BRUTE FORCE------------------------------
vector<int> Majorityby3(vector <int> nums){
    vector <int> ls;
    int count;

    for(int i = 0; i< nums.size(); i++){
        if(ls.size() == 0 || ls[0] != nums[i]){
            count = 0;
        }

        for(int j = 0; j<nums.size(); j++){
            if(nums[j] == nums[i]){
                count++;
            }
        }

        if(count > nums.size()/3){
            ls.push_back(i);
        }


        if(ls.size() > 2){
            break;
        }

    
    }

    return ls;
}



//-----------------------BETTER SOLUTION-------------------



vector <int> Majority3Better(vector <int> nums){
    vector <int> ls;
    map <int, int> mpp;
    int min = nums.size()/3 + 1;

    for(int i = 0; i< nums.size(); i++){
        mpp[nums[i]]++;

          if(mpp[nums[i]] == min){
            ls.push_back(nums[i]);

          }
    }

    return ls;

}


// ----------------------------OPTIMAL SOLUTION---------------

vector <int> MajorityElement3Optimal(vector <int> nums){
    int count1 = 0;
    int count2 = 0;
    int elem1;
    int elem2;



    for(int i = 0; i< nums.size(); i++){
        if(count1 == 0 && elem2 != nums[i]){
            count1 = 1;
            elem1 = nums[i];
        }

        else if(count2 == 0 && elem1 != nums[i]){
            count2 = 0;
            elem2 = nums[i];
        }

        else if(elem1 == nums[i]){
            count1++;
        }

        else if(elem2 == nums[i]){
            count2++;
        }

        else{
            count1--;
            count2--;
        }
    }

    vector <int> ls;
    int min = nums.size()/3 +1;
    count1 = 0;
    count2 = 0;

    for(int i = 0; i<nums.size();i++){
        if(nums[i] == elem1){
            count1++;
        }

        else if(nums[i] == elem2){
            count2++;
        }


        if(count1 >= min){
            ls.push_back(elem1);
        }

        else if(count2 >= min){
            ls.push_back(elem2);
        }
    }

    return ls;
}




int main(){
    
    return 0;
}
