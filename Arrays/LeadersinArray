#include <bits/stdc++.h>
#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

vector<int> Leader(vector <int> &arr){
    int n = arr.size();
    int maxi = INT_MIN;
    vector <int> ans;

    for(int i = n-1; i >= 0; i--){
        if(arr[i] > maxi){
            ans.push_back(arr[i]);

        }

        maxi = max(maxi, arr[i]);

       
    }

     sort(ans.begin(), ans.end());
     return ans;

}

int main(){

    int n;
    cout<<"Enter the size: ";
    cin>>n;

    vector <int> arr(n);
    cout<<"Enter the array: ";
    for(int i = 0; i<n; i++){
        cin>>arr[i];
    }

    vector <int> ans2 = Leader(arr);
     

    for(int i = 0; i<ans2.size(); i++){
        cout<<ans2[i]<<" ";
    }


}
