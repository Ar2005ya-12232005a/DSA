#include <bits/stdc++.h>
#include <iostream>
#include  <utility>
using namespace std;


//---------------------------------------BRUTE----------------------

int MaxProduct(vector <int> &nums){
    int Max = INT_MIN;
    int n = nums.size();

    for(int i = 0; i<n; i++){
        int product = 1;

        for(int j = i; j<n; j++){
            product = product * nums[j];
            Max = max(Max, product);
        }
    }

    return Max;

}

//-----------------------------OPTIMAL-----------------

int MaxProduct(vector <int> &nums){
    int prefix = 1;
    int suffix = 1;
    int Max = INT_MIN;

    int n = nums.size();


    for(int i = 0; i<n; i++){

   
    if(prefix == 0){
        prefix = 1;
    }

    if(suffix == 0){
        suffix = 1;
    }


    prefix = prefix * nums[i];
    suffix = suffix*  nums[n-i-1];
    Max = max({Max,prefix,suffix});

 }

 return Max;

    
}



int main(){
    
    return 0;
}
